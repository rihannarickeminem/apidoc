    "build:node": "cross-env NODE_ENV=production npm run babel -- ./server -d ./compiled",
    "build": "npm run clean && npm run build:prod && npm run build:node",
    "test": "cross-env NODE_ENV=test DEBUG='server:*,app:*,test:*' ./node_modules/mocha/bin/mocha --compilers js:babel-core/register --require babel-polyfill|| exit 0",
    "eslint": "./node_modules/.bin/eslint --ignore-pattern 'app/tests*' server test app",
    "start": "cross-env NODE_ENV=production node compiled/index.js"
  },
